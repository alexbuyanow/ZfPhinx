{
    "name": "alexbuyanow/zfphinx",
    "type": "library",
    "description": "Zend Framework 3 module that provides integration of Phinx library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/alexbuyanow/ZfPhinx",
    "authors": [
        {
            "name": "Alexey Buyanow",
            "email": "alexbuyanow@gmail.com"
        }
    ],
    "keywords": [
        "zf",
        "zend framework",
        "phinx",
        "database",
        "migration"
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "robmorgan/phinx": "^0.7.0",
        "zendframework/zend-db": ">=2.8",
        "zendframework/zend-servicemanager": "~3.",
        "zendframework/zend-mvc": "~3.",
        "zendframework/zend-modulemanager": ">=2.7",
        "zendframework/zend-console": ">=2.6",
        "zendframework/zend-mvc-console": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "ZfPhinx\\": "src/ZfPhinx/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfPhinx\\": "test/ZfPhinx/"
        }
    }
}
